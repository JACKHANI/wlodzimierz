{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "wlodzimierz",
  "projects": {
    "wlodzimierz": {
      "projectType": "application",
      "root": "apps/wlodzimierz",
      "sourceRoot": "apps/wlodzimierz/src",
      "prefix": "wlodzimierz",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/wlodzimierz",
            "index": "apps/wlodzimierz/src/index.html",
            "main": "apps/wlodzimierz/src/main.ts",
            "polyfills": "apps/wlodzimierz/src/polyfills.ts",
            "tsConfig": "apps/wlodzimierz/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/wlodzimierz/src/favicon.ico",
              "apps/wlodzimierz/src/assets"
            ],
            "styles": [
              "apps/wlodzimierz/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/wlodzimierz/src/environments/environment.ts",
                  "with": "apps/wlodzimierz/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wlodzimierz:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "wlodzimierz:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wlodzimierz:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/wlodzimierz/src/**/*.ts",
              "apps/wlodzimierz/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/wlodzimierz"
          ],
          "options": {
            "jestConfig": "apps/wlodzimierz/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "wlodzimierz-e2e": {
      "root": "apps/wlodzimierz-e2e",
      "sourceRoot": "apps/wlodzimierz-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/wlodzimierz-e2e/cypress.json",
            "tsConfig": "apps/wlodzimierz-e2e/tsconfig.e2e.json",
            "devServerTarget": "wlodzimierz:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wlodzimierz:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/wlodzimierz-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "home": {
      "projectType": "library",
      "root": "libs/home",
      "sourceRoot": "libs/home/src",
      "prefix": "wlodzimierz",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/home/src/**/*.ts",
              "libs/home/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/home"
          ],
          "options": {
            "jestConfig": "libs/home/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "docs": {
      "projectType": "library",
      "root": "libs/docs",
      "sourceRoot": "libs/docs/src",
      "prefix": "wlodzimierz",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/docs/src/**/*.ts",
              "libs/docs/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/docs"
          ],
          "options": {
            "jestConfig": "libs/docs/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "users": {
      "projectType": "library",
      "root": "libs/users",
      "sourceRoot": "libs/users/src",
      "prefix": "wlodzimierz",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/users/src/**/*.ts",
              "libs/users/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/users"
          ],
          "options": {
            "jestConfig": "libs/users/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "statuses": {
      "projectType": "library",
      "root": "libs/statuses",
      "sourceRoot": "libs/statuses/src",
      "prefix": "wlodzimierz",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/statuses/src/**/*.ts",
              "libs/statuses/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/statuses"
          ],
          "options": {
            "jestConfig": "libs/statuses/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "chat": {
      "projectType": "library",
      "root": "libs/chat",
      "sourceRoot": "libs/chat/src",
      "prefix": "wlodzimierz",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/chat/src/**/*.ts",
              "libs/chat/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/chat"
          ],
          "options": {
            "jestConfig": "libs/chat/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
